Extension { #name : #FAMIXReference }

{ #category : #'*OrionFamixCore-Class' }
FAMIXReference >> privateSource [
	^ source
]

{ #category : #'*OrionFamixCore-Class' }
FAMIXReference >> privateSource: aContainerEntity [
	 source := aContainerEntity
]

{ #category : #'*OrionFamixCore-Class' }
FAMIXReference >> privateTarget [
	^ target
]

{ #category : #'*OrionFamixCore-Class' }
FAMIXReference >> privateTarget: aContainerEntity [
	 target := aContainerEntity
]
