Extension { #name : #FAMIXMethod }

{ #category : #'*OrionCore-model' }
FAMIXMethod >> privateKind [
	^ kind
]

{ #category : #'*OrionCore-model' }
FAMIXMethod >> privateKind: aString [
	 kind := aString
]

{ #category : #'*OrionCore-model' }
FAMIXMethod >> privateParentType [
	^ parentType
]

{ #category : #'*OrionCore-model' }
FAMIXMethod >> privateParentType: aType [
	 parentType := aType
]
